<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.topnav {
			color: white;
		}
		
		.title{
			font-weight:bold;
			font-size:1.5em;
			padding: 0em 2em;
		}
		
		.center {
			background-color: white;
			padding:1em;
		}
		
	</ui:style>
	<g:DockLayoutPanel unit="EM">
		<g:north size="2.4">
			<g:FlowPanel styleName="{style.topnav}">
				<g:InlineHyperlink text="&nbsp;&lt;- innapoi la orar " targetHistoryToken="orar"/>
				<g:InlineLabel ui:field="title" styleName="{style.title}"/>
			</g:FlowPanel>
		</g:north>
		<g:center>
			<g:HTML wordWrap="true" ui:field="materialeHTML" styleName="{style.center}">
			</g:HTML>
		</g:center>

	</g:DockLayoutPanel>
	<!-- <g:TabLayoutPanel barHeight="3" barUnit="EM" width="100%" height="100%" 
		ui:field="tabpanel" styleName="tabpanelnav"> <g:tab> <g:header>Materiale</g:header> 
		<g:HTML wordWrap="true" ui:field="materialeHTML"> </g:HTML> </g:tab> </g:TabLayoutPanel> -->
</ui:UiBinder> 