<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.wrap{
			position: relative;		
			width: 100%;
			height: 100%;
			background-color:white;
		}
		
		.grid {
			width:100%;
			height:100%;
			border-collapse: collapse;
			text-align: center;
			table-layout: fixed;
		}
		
		.grid td {
			border-right: solid 1px #0095FF;
			border-bottom: dashed 1px #EEEEEE;
			width: 12.5%;
		}
		
		.head td{
			border-bottom: solid 1px #0095FF;
			font-weight:bold;
		}
		.even td{
			border-bottom: solid 1px #EEEEEE;
		}
	</ui:style>
		
	<g:HTMLPanel ui:field="weekWrap" styleName="{style.wrap}">
		<table class="{style.grid}">
			<tbody>
			<tr class="{style.head}"><td>Luni</td><td>Marti</td><td>Miercuri</td><td>Joi</td><td>Vineri</td></tr>
			<tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr class="{style.even}"><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr class="{style.even}"><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr class="{style.even}"><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr class="{style.even}"><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr class="{style.even}"><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			<tr class="{style.even}"><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>
			</tbody>
		</table>
	</g:HTMLPanel>
		
</ui:UiBinder> 
