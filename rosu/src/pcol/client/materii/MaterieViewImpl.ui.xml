<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.grid{
			width:100%;
			height:100%;
			border-collapse: collapse;
			text-align:center;
		}
		
		.grid td{
			border-bottom: dashed 1px #EEEEEE;
			border-right: solid 1px #CCCCCC;
			width:12.5%;
		}
		
	</ui:style>
	<g:TabLayoutPanel barHeight="3" barUnit="EM" width="100%"
		height="100%" ui:field="tabpanel" styleName="tabpanelnav">
		<g:tab>
			<g:header>Orar</g:header>
			<g:Grid ui:field="weekGrid" styleName="{style.grid}">
				<g:row>
					<g:cell>
					</g:cell>
					<g:cell> L </g:cell>
					<g:cell> M </g:cell>
					<g:cell> M </g:cell>
					<g:cell> J </g:cell>
					<g:cell> V </g:cell>
					<g:cell> S </g:cell>
					<g:cell> D </g:cell>
				</g:row>
				<g:row><g:cell>8-9</g:cell></g:row>
				<g:row><g:cell>9-10</g:cell></g:row>
				<g:row><g:cell>10-11</g:cell></g:row>
				<g:row><g:cell>11-12</g:cell></g:row>
				<g:row><g:cell>12-13</g:cell></g:row>
				<g:row><g:cell>13-14</g:cell></g:row>
				<g:row><g:cell>14-15</g:cell></g:row>
				<g:row><g:cell>15-16</g:cell></g:row>
				<g:row><g:cell>16-17</g:cell></g:row>
				<g:row><g:cell>17-18</g:cell></g:row>
				<g:row><g:cell>18-19</g:cell></g:row>
				<g:row><g:cell>19-20</g:cell></g:row>
			</g:Grid>
		</g:tab>
		<g:tab>
			<g:header>Materiale</g:header>
			<g:HTML wordWrap="true" ui:field="materialeHTML">
			</g:HTML>
		</g:tab>
	</g:TabLayoutPanel>
</ui:UiBinder> 